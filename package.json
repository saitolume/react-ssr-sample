{
  "name": "react-ssr-sample",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "saitoeku3",
    "email": "saitoeku3@gmail.com"
  },
  "scripts": {
    "build": "yarn rm:dist && yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "build:server": "tsc -p tsconfig.server.json",
    "rm:dist": "rimraf dist",
    "start": "ts-node ./src/server/index.ts",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server/server/index.js"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "express": "^4.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.16",
    "@types/webpack-env": "^1.13.9",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  }
}
